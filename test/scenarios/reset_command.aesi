":load test/contracts/c.aes".
"let x = 1".
"let y = 2".
"type al = int".
"datatype t = T".
"include \"List.aes\"".
":set call_value 100".

{ ":lookup vars"
, { match
  , [ {"f", {fun_t, '_', [], [{id, '_', "int"}], {id, '_', "int"}}}
    , {"y", {id, '_', "int"}}
    , {"x", {id, '_', "int"}}
    ]
  }
}.

{ ":lookup types"
, {match, ["al", "t"]}
}.

{ ":lookup includes"
, {match, ["List.aes", "test/contracts/c.aes"]}
}.

{ ":lookup files"
, {match, ["test/contracts/c.aes"]}
}.

{ ":lookup options"
, { match
  , {options,
     #{ call_gas => 100000000000000000
      , call_value => 100
      , print_gas => false
      , print_format => sophia
      , print_unit => false
      }}
  }
}.

":reset".

{ ":lookup vars"
, {match, []}
}.

{ ":lookup types"
, {match, []}
}.

{ ":lookup includes"
, {match, []}
}.

{ ":lookup files"
, {match, []}
}.

{ ":lookup options"
, { match
  , {options,
     #{ call_gas => 100000000000000000
      , call_value => 100
      , display_gas => false
      , print_format => sophia
      , print_unit => false
      }}
  }
}.
