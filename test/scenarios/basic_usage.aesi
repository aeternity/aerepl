"type t1 = int".
"type t2('a) = list('a)".
"type t3('a) = t2('a)".
"let e1 = 1".
"let [e2, e3] : t3(t1) = [2, 3]".

{ "e1 + e2 + e3"
, {match, {int, '_', 6}}
}.

"include \"List.aes\"".

{ "List.map((x) => x + 1, [1, 2, 3])"
, {match, {list, '_', [{int, '_', 2},{int, '_', 3},{int, '_', 4}]}}
}.

