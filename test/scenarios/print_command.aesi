"datatype d = Int".

"datatype d = Char".

"datatype d = String".

{ ":lookup types"
, {expect, ["d"]}
}.

"let x = 'c'".

{ ":lookup vars"
, {match, {vars, [{"x", {id, '_', "char"}}]}}
}.

"let x = 1".

{ ":lookup vars"
, {match, {vars, [{"x", {id, '_', "int"}}]}}
}.

"let y = 2".

{ ":lookup vars"
, {match, {vars, [{"x", {id, '_', "int"}}, {"y", {id, '_', "int"}}]}}
}.

"let y = \"string\"".

{ ":lookup vars"
, {match, {vars, [{"x", {id, '_', "int"}}, {"y", {id, '_', "string"}}]}}
}.
